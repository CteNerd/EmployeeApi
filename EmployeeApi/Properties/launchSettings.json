{
  "$schema": "http://json.schemastore.org/launchsettings.json",
  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "System": "Information",
      "Microsoft": "Information"
    }
  },
  "profiles": {
    "Production": {
      "commandName": "Project",
      "launchBrowser": true,
      "launchUrl": "http://localhost/swagger/index.html",
      "applicationUrl": "https://localhost:8081;http://localhost:8080",
      "environmentVariables": {
        "SENDGRIDAPIKEY": "mail",
        "MONGOCONNECTIONSTRING": "ProdDB",
        "JWTTOKENSIGNATUREKEY": "secrets",
        "ASPNETCORE_ENVIRONMENT": "Production"
      }
    },
    "Development": {
      "commandName": "Project",
      "launchBrowser": true,
      "launchUrl": "http://localhost/swagger/index.html",
      "applicationUrl": "https://localhost:53685;http://localhost:43673",
      "environmentVariables": {
        "SENDGRIDAPIKEY": "Mail",
        "MONGOCONNECTIONSTRING": "Development",
        "JWTTOKENSIGNATUREKEY": "secrets",
        "ASPNETCORE_ENVIRONMENT": "Development"
      }
    }
  }
